{
  "name": "web-portfolio",
  "version": "3.0.0",
  "description": "Personal portfolio of web work",
  "main": "src/index.js",
  "scripts": {
    "dev": "clear && webpack --mode development --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "build": "clear && webpack --mode production --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karencalpo/web-portfolio"
  },
  "keywords": [
    "portfolio",
    "web",
    "React",
    "JavaScript"
  ],
  "author": "Karen Calpo <minnkc@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/karencalpo/web-portfolio/issues"
  },
  "homepage": "https://github.com/karencalpo/web-portfolio#readme",
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^2.1.5"
  }
}
