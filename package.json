{
  "name": "web-portfolio",
  "version": "3.0.1",
  "description": "Personal portfolio of web work",
  "main": "src/index.js",
  "scripts": {
    "dev": "clear && webpack --mode development --module-bind js=babel-loader --progress",
    "build": "clear && webpack --mode production --module-bind js=babel-loader --progress",
    "test": "jest",
    "jest-init": "jest --init",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "nrebuild": "npm run clean && npm install && npm run build",
    "start-dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "start": "node ./service/index.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransformer.js",
      "^.+\\.js$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karencalpo/web-portfolio"
  },
  "keywords": [
    "portfolio",
    "web",
    "React",
    "JavaScript"
  ],
  "author": "Karen Calpo <minnkc@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/karencalpo/web-portfolio/issues"
  },
  "homepage": "https://github.com/karencalpo/web-portfolio#readme",
  "dependencies": {
    "react-scroll": "^1.7.10",
    "save-dev": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "compression": "^1.7.3",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "mockery": "^2.1.0",
    "node-sass": "^4.9.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.8"
  }
}
