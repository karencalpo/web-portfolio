{
  "name": "web-portfolio",
  "version": "3.0.1",
  "description": "Personal portfolio of web work",
  "main": "src/index.js",
  "scripts": {
    "dev": "clear && webpack --mode development --module-bind js=babel-loader --progress",
    "build": "clear && webpack --mode production --module-bind js=babel-loader --progress",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "start-dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "start": "node ./service/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karencalpo/web-portfolio"
  },
  "keywords": [
    "portfolio",
    "web",
    "React",
    "JavaScript"
  ],
  "author": "Karen Calpo <minnkc@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/karencalpo/web-portfolio/issues"
  },
  "homepage": "https://github.com/karencalpo/web-portfolio#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "compression": "^1.7.3",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.8"
  }
}
